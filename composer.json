{
  "name": "pulkitjalan/paypal-laravel",
  "description": "A Laravel 5 wrapper for PayPal",
  "homepage": "https://github.com/pulkitjalan/paypal-laravel",
  "keywords": ["paypal", "laravel"],
  "license": "MIT",
  "authors": [
    {
      "name": "Pulkit Jalan"
    }
  ],
  "require": {
    "php": ">=5.5",
    "illuminate/support": "~6.0",
    "paypal/rest-api-sdk-php": "*"
  },
  "autoload": {
    "psr-4": {
      "PulkitJalan\\PayPal\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "PulkitJalan\\PayPal\\PayPalServiceProvider"
      ],
      "aliases": {
        "PayPal": "PulkitJalan\\PayPal\\Facades\\PayPal"
      }
    }
  }
}
